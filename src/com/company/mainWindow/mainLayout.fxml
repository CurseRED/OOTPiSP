<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="480.0" prefWidth="640.0" spacing="8.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.mainWindow.MainController">
   <children>
      <Label text="Список объектов" textAlignment="CENTER" />
      <ListView VBox.vgrow="ALWAYS">
         <opaqueInsets>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </opaqueInsets>
      </ListView>
      <HBox alignment="CENTER_RIGHT" spacing="8.0">
         <children>
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#onClickAddButton" text="Добавить" />
            <Button mnemonicParsing="false" text="Редактировать" />
            <Button mnemonicParsing="false" text="Удалить" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </HBox>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
